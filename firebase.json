{
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "trailingSlash": true,
    "rewrites": [
      {
        "source": "/",
        "function": "root"
      },
      {
        "source": "/en/**",
        "destination": "/en/404.html"
      },
      {
        "source": "/ja/**",
        "destination": "/ja/404.html"
      }
    ]
  }
}
